World {
  SimpleCamera cameraA {
    Function lookAt (
      position = [ 0.0 0.0 0.0 ]
      target = [ 0.0 0.0 1.0 ]
      up = [ 0.0 1.0 0.0 ]
    )
  }

  Sphere sphereB {
    Function translate (
      position = [ 0.0 0.0 5.0 ]
    )

    radius = 1.0
    zMax = 1.0
    zMin = 0.0
    phiMax = 360
  }
}